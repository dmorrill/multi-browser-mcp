{
  "name": "blueprint-mcp-monorepo",
  "version": "1.5.5",
  "private": true,
  "description": "Blueprint MCP monorepo - MCP server and browser extensions",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/railsblueprint/blueprint-mcp.git"
  },
  "homepage": "https://blueprint-mcp.railsblueprint.com",
  "author": {
    "name": "Rails Blueprint"
  },
  "license": "Apache-2.0",
  "scripts": {
    "install:server": "cd server && npm install",
    "install:chrome": "cd extensions/chrome && npm install",
    "install:all": "npm run install:server && npm run install:chrome",
    "build:chrome": "cd extensions/chrome && npm run build",
    "test:server": "cd server && npm test",
    "dev:server": "cd server && node cli.js --debug"
  }
}
