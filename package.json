{
  "name": "blueprint-mcp-monorepo",
  "version": "1.5.5",
  "private": true,
  "description": "Blueprint MCP monorepo - MCP server and browser extensions",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/railsblueprint/blueprint-mcp.git"
  },
  "homepage": "https://blueprint-mcp.railsblueprint.com",
  "author": {
    "name": "Rails Blueprint"
  },
  "license": "Apache-2.0",
  "workspaces": [
    "server",
    "extensions/chrome"
  ],
  "scripts": {
    "install:all": "npm install && npm install --workspace=server && npm install --workspace=extensions/chrome",
    "build:chrome": "npm run build --workspace=extensions/chrome",
